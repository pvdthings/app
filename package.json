{
  "name": "things-app",
  "description": "The gateway to your Library of Things.",
  "version": "0.1.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev npm-run-all --parallel dev:app dev:server",
    "dev:app": "sapper dev",
    "dev:server": "netlify-lambda serve functions",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "netlify:build": "run-p netlify:build:**",
    "netlify:build:app": "sapper export",
    "netlify:build:lambda": "netlify-lambda build functions"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "express": "^4.17.1",
    "netlify-lambda": "^2.0.11",
    "node-fetch": "^2.6.1",
    "polka": "^1.0.0-next.15",
    "sirv": "^1.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.14.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.0.2",
    "http-proxy-middleware": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-nesting": "^7.0.1",
    "rollup": "^2.53.1",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.29.3",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^4.7.4",
    "tailwindcss": "^2.2.4"
  }
}
